{
  "name": "monym",
  "version": "0.0.1",
  "description": "Monitor Your Media is a webapp that is designed to integrate specific aspects of Sonarr, Radarr, and Lidarr to the users who use them. Monym is designed to give limited access to end users.",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "nodemon --watch public/ --watch src/ src/index.js",
    "scss": "node-sass sass/ -o public/",
    "docker-build": "docker build -t glenn/monym"
  },
  "keywords": [
    "Sonarr",
    "Radarr",
    "Lidarr",
    "Monitor",
    "End-User",
    "Request",
    "API"
  ],
  "author": "GlennCottam",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.7.9",
    "cors": "^2.8.5",
    "ejs": "^3.0.1",
    "express": "^4.17.3",
    "fs": "0.0.1-security",
    "jquery": "^3.4.1",
    "node-sass": "^4.13.1",
    "nodejs": "0.0.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
